# Development Environment Configuration for Influmojo Backend

# Server Configuration
NODE_ENV=development
PORT=3002

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://localhost:8081

# Database Configuration
DATABASE_URL=your_development_database_url_here

# JWT Configuration
JWT_SECRET=dev_jwt_secret_key_here

# AWS Configuration
AWS_REGION=us-east-1

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_VERIFY_SERVICE_SID=your_twilio_verify_service_sid_here

# Google OAuth
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Facebook OAuth
FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# SendGrid Configuration
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Stream Chat Configuration
STREAM_API_SECRET=your_stream_api_secret_here
