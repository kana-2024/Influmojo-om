[32m+[m[32m// Ticket API calls (matches mobile app implementation)[m
[32m+[m[32mexport const ticketAPI = {[m
[32m+[m[32m  // Get messages for a specific ticket[m
[32m+[m[32m  getTicketMessages: async (ticketId: string, loadOlderMessages: boolean = false) => {[m
[32m+[m[32m    const queryParams = loadOlderMessages ? '?loadOlderMessages=true' : '';[m
[32m+[m[32m    return await apiRequest(`${ENV.API_BASE_URL}/api/crm/tickets/${ticketId}/messages${queryParams}`, {[m
[32m+[m[32m      method: 'GET',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Send message to a ticket[m
[32m+[m[32m  sendTicketMessage: async (ticketId: string, messageData: {[m
[32m+[m[32m    message_text: string;[m
[32m+[m[32m    sender_role?: 'brand' | 'creator' | 'agent' | 'system';[m
[32m+[m[32m    channel_type?: 'brand_agent' | 'creator_agent';[m
[32m+[m[32m    message_type?: 'text' | 'file' | 'system';[m
[32m+[m[32m  }) => {[m
[32m+[m[32m    return await apiRequest(`${ENV.API_BASE_URL}/api/crm/tickets/${ticketId}/messages`, {[m
[32m+[m[32m      method: 'POST',[m
[32m+[m[32m      body: JSON.stringify(messageData),[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Get ticket by order ID[m
[32m+[m[32m  getTicketByOrderId: async (orderId: string) => {[m
[32m+[m[32m    return await apiRequest(`${ENV.API_BASE_URL}/api/crm/tickets/order/${orderId}`, {[m
[32m+[m[32m      method: 'GET',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Update ticket status[m
[32m+[m[32m  updateTicketStatus: async (ticketId: string, status: string) => {[m
[32m+[m[32m    return await apiRequest(`${ENV.API_BASE_URL}/api/crm/tickets/${ticketId}/status`, {[m
[32m+[m[32m      method: 'PUT',[m
[32m+[m[32m      body: JSON.stringify({ status }),[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
