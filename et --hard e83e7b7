[32m+[m[32m// Orders API calls (matches mobile app implementation)[m
[32m+[m[32mexport const ordersAPI = {[m
[32m+[m[32m  // Get all orders[m
[32m+[m[32m  getOrders: async () => {[m
[32m+[m[32m    return await apiRequest(API_ENDPOINTS.GET_ORDERS, {[m
[32m+[m[32m      method: 'GET',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Get specific order details[m
[32m+[m[32m  getOrderDetails: async (orderId: string) => {[m
[32m+[m[32m    return await apiRequest(`${API_ENDPOINTS.GET_ORDER_DETAILS}/${orderId}`, {[m
[32m+[m[32m      method: 'GET',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Checkout orders from cart (matches mobile app payload structure)[m
[32m+[m[32m  checkoutOrders: async (cartItems: {[m
[32m+[m[32m    packageId: string;[m
[32m+[m[32m    creatorId: string;[m
[32m+[m[32m    quantity: number;[m
[32m+[m[32m    deliveryTime?: number;[m
[32m+[m[32m    additionalInstructions?: string;[m
[32m+[m[32m    references?: string[];[m
[32m+[m[32m  }[]) => {[m
[32m+[m[32m    return await apiRequest(API_ENDPOINTS.CHECKOUT_ORDERS, {[m
[32m+[m[32m      method: 'POST',[m
[32m+[m[32m      body: JSON.stringify({ cartItems }),[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Accept order (creators only)[m
[32m+[m[32m  acceptOrder: async (orderId: string) => {[m
[32m+[m[32m    if (!orderId || orderId === 'undefined' || orderId === 'null') {[m
[32m+[m[32m      throw new Error('Invalid order ID provided');[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    try {[m
[32m+[m[32m      const response = await apiRequest(`${API_ENDPOINTS.GET_ORDERS}/${orderId}/accept`, {[m
[32m+[m[32m        method: 'PUT',[m
[32m+[m[32m      });[m
[32m+[m[41m      [m
[32m+[m[32m      if (!response.success) {[m
[32m+[m[32m        throw new Error(response.message || 'Failed to accept order');[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      return response;[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error('[apiService] Error accepting order:', error);[m
[32m+[m[32m      throw new Error(error && typeof error === 'object' && 'message' in error ? String(error.message) : 'Failed to accept order. Please try again.');[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Reject order (creators only)[m
[32m+[m[32m  rejectOrder: async (orderId: string, rejectionMessage?: string) => {[m
[32m+[m[32m    return await apiRequest(`${API_ENDPOINTS.GET_ORDERS}/${orderId}/reject`, {[m
[32m+[m[32m      method: 'PUT',[m
[32m+[m[32m      body: JSON.stringify({ rejectionMessage }),[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Get chat information for an order[m
[32m+[m[32m  getOrderChat: async (orderId: string) => {[m
[32m+[m[32m    return await apiRequest(`${API_ENDPOINTS.GET_ORDERS}/${orderId}/chat`, {[m
[32m+[m[32m      method: 'GET',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // Enable chat for an existing order[m
[32m+[m[32m  enableOrderChat: async (orderId: string) => {[m
[32m+[m[32m    return await apiRequest(`${API_ENDPOINTS.GET_ORDERS}/${orderId}/enable-chat`, {[m
[32m+[m[32m      method: 'POST',[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
